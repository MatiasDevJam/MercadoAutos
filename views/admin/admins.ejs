<!DOCTYPE html>
<html lang="en">

<%- include('./partials/head') %> 

<body>
    <%- include('./partials/header') %> 

    <main>
        <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">username</th>
                <th scope="col">pass</th>
                
              </tr>
            </thead>
            <tbody>
                <% admins.forEach(element => { %>
                    <tr>
                        <th scope="row"><<%= admin.id %> </th>
                        <td><%= admin.username %></td>
                        <td><%= admin.pass %></td>
                        <td><a href="/admin/profile/<%= admin.id%>" class="btn btn-sm btn-primary"></a></td>
                        
                      </tr>
                    
                <% }) %>
            </tbody>
          </table>
        
    </main>

    <%- include('./partials/footer') %> 
</body>
</html>